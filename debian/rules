#!/usr/bin/make -f

export JAVA_HOME=/usr/lib/jvm/default-java
export CLASSPATH=/usr/share/java/healpix.jar:/usr/share/java/cds.moc.jar:/usr/share/java/cds.savot.jar:/usr/share/java/jsamp.jar:/usr/share/java/kxml2.jar:/usr/share/java/ws-commons-util.jar:/usr/share/java/commons-logging.jar:/usr/share/java/commons-codec.jar:/usr/share/java/commons-httpclient.jar:/usr/share/java/servlet-api-3.1.jar

%:
	dh $@ --with javahelper 

override_dh_auto_build:
	ant main
